<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.airbnb.mappers.LikesMapper">
<insert id="insertLike" parameterType="Like" useGeneratedKeys="true" keyProperty="id">
    insert into likes (account_id, accommodation_id)
    values (#{accountId}, #{accommodationId});
</insert>



</mapper>